@if (Request.QueryString["login"] != null && Request.QueryString["login"].ToString().Trim() == "verified")
{
<script type="text/javascript">
    
    $(".login_container").fadeIn(50);
    $(".login_container").css("display", "inline-block");
    $(".signup_container").css("display", "none");
    $(".request_container").css("display", "none");
    $('.sub_menumob').css("z-index", "0");
    $('.sub_menumob').css("opacity", "0");
    $('.sub_menumob').css("transition", " 0.05s ease");
    $('.sub_menumob').css("top", "-2000px");
    $('.sub_menumob').css("transition", " 0.05s ease");
</script>
}
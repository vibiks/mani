                <input id="ship_country" type="text" name="Username" placeholder="Destination Country" />
                <input id="ship_weight" type="text" name="Username" placeholder="Weight in Kg (ex: 0.5,1,1.5)" />
                <input id="ship_product" type="text" name="Username" placeholder="Product" />
                <input id="ship_email" type="email" name="Email" placeholder="Email ID" />
                <input id="ship_mobile" type="number" name="Mobile" placeholder="Mobile number" />
